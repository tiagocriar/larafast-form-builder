{
    "name": "fastponto/larafast-form-builder",
    "description": "Larafast Form Builder",
    "keywords": ["larafast", "laravel", "form", "builder","symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kristijan Husak",
            "email": "husakkristijan@gmail.com"
        },
        {
            "name": "Tiago Oliveira",
            "email": "tiago.oliveira@fastponto.com.br"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravelcollective/html": "^5.6|^6|^7|^8",
        "illuminate/database": "^5.6@dev|^6|^7|^8",
        "illuminate/validation": "^5.6@dev|^6|^7|^8"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.16.x-dev"
        },
        "laravel": {
            "providers": [
                "Fastponto\\LaravelFormBuilder\\FormBuilderServiceProvider"
            ],
            "aliases": {
                "FormBuilder": "Fastponto\\LaravelFormBuilder\\Facades\\FormBuilder"
            }
        }
    },
    "autoload": {
        "psr-0": {
            "Fastponto\\LaravelFormBuilder": "src/"
        },
        "classmap": [
            "tests/FormBuilderTestCase.php"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
